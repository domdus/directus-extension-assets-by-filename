{
	"name": "directus-extension-assets-by-filename",
	"description": "Retrieve files by filename_disk instead of UID",
	"icon": "image",
	"version": "1.0.1",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint",
		"assets-by-filename",
		"assets",
		"filename"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.0.0",
		"sandbox": {
			"enabled": true,
			"requestedScopes": {
				"log": {},
				"request": {
					"methods": ["GET"],
					"urls": [
						"http://localhost:*/*",
						"http://127.0.0.1:*/*",
						"https://localhost:*/*",
						"https://127.0.0.1:*/*"
					]
				}
			}
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/domdus/directus-extension-assets-by-filename.git"
	},
	"bugs": {
		"url": "https://github.com/domdus/directus-extension-assets-by-filename/issues"
	},
	"homepage": "https://github.com/domdus/directus-extension-assets-by-filename#readme",
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^17.0.0",
		"@directus/types": "^12.0.0",
		"@types/express": "^4.17.21",
		"@types/node": "^20.0.0",
		"typescript": "^5.0.0"
	}
}
