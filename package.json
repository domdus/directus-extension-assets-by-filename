{
	"name": "directus-extension-assets-by-filename",
	"description": "Retrieve files by filename_disk instead of UID",
	"icon": "image",
	"version": "1.0.0",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint",
		"assets-by-filename",
		"assets",
		"filename"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.0",
		"@directus/types": "^12.0.0",
		"@types/express": "^4.17.21",
		"@types/node": "^20.0.0",
		"typescript": "^5.0.0"
	}
}
